@article{Yardi_Romero_Schoenebeck_Boyd_2010, title={Detecting spam in a Twitter network}, volume={15}, url={http://firstmonday.org/htbin/cgiwrap/bin/ojs/index.php/fm/article/view/2793/2431}, abstractNote={Spam becomes a problem as soon as an online communication medium becomes popular. Twitters behavioral and structural properties make it a fertile breeding ground for spammers to proliferate. In this article we examine spam around a onetime Twitter meme robotpickuplines. We show the existence of structural network differences between spam accounts and legitimate users. We conclude by highlighting challenges in disambiguating spammers from legitimate users.}, number={1}, journal={First Monday}, author={Yardi, Sarita and Romero, Daniel and Schoenebeck, Grant and Boyd, Danah M}, year={2010}, pages={1-13}}

